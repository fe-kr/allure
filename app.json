{
  "expo": {
    "name": "Allure",
    "slug": "Allure",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "Allure",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "package": "com.mjk.allure"
    },
    "android": {
      "package": "com.mjk.allure"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
